<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="&lt;br /&gt;\s+" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<br />\n" param2=""/>
</dwquery>
