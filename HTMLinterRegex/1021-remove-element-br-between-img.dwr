<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="&lt;img(( [a-z]+=&quot;(.+)&quot;)*) /&gt;((\s*&lt;br /&gt;)+)\s*&lt;img(( [a-z]+=&quot;(.+)&quot;)*) /&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<img$1 />\n<img$6 />" param2=""/>
</dwquery>
