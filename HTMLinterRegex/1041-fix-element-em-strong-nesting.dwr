<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="&lt;em&gt;&lt;strong&gt;(.+)&lt;/strong&gt;&lt;/em&gt;&lt;strong&gt;(.+)&lt;/strong&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<strong><em>$1</em>$2</strong>" param2=""/>
</dwquery>
