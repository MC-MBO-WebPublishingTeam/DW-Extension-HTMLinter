<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="(&lt;p&gt;&lt;a name=&quot;note\d+&quot;&gt;&lt;sup&gt;\d+&lt;/sup&gt;&lt;/a&gt;.*)\\" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="$1" param2=""/>
</dwquery>
