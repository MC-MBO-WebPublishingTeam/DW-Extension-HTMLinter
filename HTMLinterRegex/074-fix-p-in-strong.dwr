<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="&lt;strong&gt;&lt;p&gt;(.+)&lt;/p&gt;&lt;/strong&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<p><strong>$1</strong></p>" param2=""/>
</dwquery>
