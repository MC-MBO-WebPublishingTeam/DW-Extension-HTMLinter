    <qtext qname="&lt;/p&gt;((\s*&lt;br /&gt;)+)\s*&lt;p&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="</p>\n<p>" param2=""/>
</dwquery>
