<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="&lt;td( [a-z]+=&quot;[^&quot;]+&quot;)*&gt;\s*&lt;/td&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<td$1>&amp;nbsp;</td>" param2=""/>
</dwquery>
