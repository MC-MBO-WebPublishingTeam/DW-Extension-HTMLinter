<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find>
    <qtext qname="&lt;p&gt;&lt;a name=&quot;note(\d+)&quot;&gt;&lt;sup&gt;(\d+)&lt;/sup&gt;&lt;/a&gt; &lt;p&gt;(.+)&lt;/p&gt;&lt;/p&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<p><a name=&quot;note$1&quot;><sup>$2</sup></a> $3</p>" param2=""/>
</dwquery>
