<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="\s*&lt;h([1-6])&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="\n\n<h$1>" param2=""/>
</dwquery>
