<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find>
    <qtext qname="&lt;p&gt;(.*)&lt;div&gt;&lt;img(( [a-z]+=&quot;(.+)&quot;)*) /&gt;&lt;/div&gt;(.*)&lt;/p&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<p>$1<img$2 />$5</p>" param2=""/>
</dwquery>
