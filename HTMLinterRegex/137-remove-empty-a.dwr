<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find>
    <qtext qname="&lt;a ([a-z]+=&quot;[^&quot;]+&quot;)*&gt;(\s*)&lt;/a&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="$2" param2=""/>
</dwquery>
