<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="((\s*&lt;p&gt;\s*(-|&bull;|&#8729;|&middot;|&Yuml;|&sect;\&#8226;)\s*(.+)&lt;/p&gt;)+)" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="\n<ul>$1\n</ul>" param2=""/>
</dwquery>
