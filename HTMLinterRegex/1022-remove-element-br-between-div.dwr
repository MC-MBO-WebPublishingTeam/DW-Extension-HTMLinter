<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="&lt;/div&gt;((\s*&lt;br /&gt;)+)\s*&lt;div&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="</div>\n<div>" param2=""/>
</dwquery>
