<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find searchmode="AllOpenDocuments">
    <qtext qname="&lt;strong&gt;([^&lt;]*)\(([^&lt;]+)&lt;/strong&gt;\)" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<strong>$1($2)</strong>" param2=""/>
</dwquery>
