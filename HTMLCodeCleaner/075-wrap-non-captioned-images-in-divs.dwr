<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find>
    <qtext qname="&lt;p&gt;((\s*&lt;img(( [a-z]+=&quot;(.+)&quot;)*) /&gt;)+)\s*&lt;/p&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<div>$1</div>" param2=""/>
</dwquery>
