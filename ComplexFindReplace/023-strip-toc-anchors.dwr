<?xml version="1.0"?><dwquery>  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />  <find searchmode="document">    <qtag qname="a" qconvertednls="true"><qattribute qname="name" qcompare="=" qvalue="TOC\d+_\d+"></qattribute></qtag>  </find>  <replace action="stripTag" param1="name" param2="1"/></dwquery>