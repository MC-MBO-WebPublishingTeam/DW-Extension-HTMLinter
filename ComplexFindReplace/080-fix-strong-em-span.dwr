<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="true" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find>
    <qtext qname="&lt;em&gt;(\s*)&lt;strong&gt;(.+)&lt;/strong&gt;(\s*)&lt;/em&gt;(\s*)&lt;strong&gt;(.+)&lt;/strong&gt;" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="<strong><em>$1$2$3</em>$4$5</strong>" param2=""/>
</dwquery>
